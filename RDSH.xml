<Configuration>
    <Add OfficeClientEdition="64" Channel="Monthly">
        <Product ID="O365ProPlusRetail">
            <Language ID="fr-fr"/>
	        <ExcludeApp ID="InfoPath"/>
        </Product>
    </Add>
